{
  "compilerOptions": {
    "useDefineForClassFields": true,
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "moduleDetection": "force",
    "strict": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}
